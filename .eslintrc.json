{
    "extends": [
        "airbnb-base"
    ],
    "rules": {
        "indent": ["error", 4],
        "no-var": "error",
        "comma-dangle": ["error", "never"],
        "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
        "no-param-reassign": ["error", { "props": false }],
        "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }]
    },
    "env": {
        "browser": true,
        "jasmine": true
    }
}
